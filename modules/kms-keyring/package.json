    "tslib": "^2.2.0"
  },
  "sideEffects": false,
  "main": "./build/main/src/index.js",
  "module": "./build/module/src/index.js",
  "types": "./build/main/src/index.d.ts",
  "files": [
    "build/**/src/*"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:symon-ai/aws-encryption-sdk-javascript.git",
